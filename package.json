{
  "name": "d2l-fetch",
  "description": "Provides support for wrapping window.fetch calls within middleware functions",
  "main": "index.js",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "bower install",
    "build": "gulp",
    "pretest": "npm run build",
    "test:lint": "eslint src/ test/ --ext .js",
    "test:no-lint": "cross-env LAUNCHPAD_BROWSERS=firefox wct --skip-plugin sauce -p",
    "test": "npm run test:lint && npm run test:no-lint",
    "publish-location": "node publishLocation | peanut-gallery -t $GITHUB_RELEASE_TOKEN -p",
    "prepublish-release": "npm run build",
    "publish-release": "frau-publisher && npm run publish-location"
  },
  "config": {
    "frauPublisher": {
      "files": "./dist/**",
      "moduleType": "lib",
      "targetDirectory": "d2lfetch",
      "creds": {
        "key": "AKIAJYZ3X3B3O6UZIFAQ",
        "secretVar": "CDN_SECRET"
      },
      "devTagVar": "TRAVIS_COMMIT",
      "versionVar": "TRAVIS_TAG"
    }
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babelify": "^7.3.0",
    "bower": "^1.8.0",
    "browserify": "^14.4.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-brightspace": "^0.2.4",
    "eslint-plugin-html": "^2.0.1",
    "frau-publisher": "^2.6.2",
    "gulp": "^3.9.1",
    "gulp-change": "^1.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "peanut-gallery": "^1.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "web-component-tester": "^6.0.0"
  },
  "dependencies": {
    "frau-jwt": "^2.0.0"
  }
}
